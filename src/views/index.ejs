<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"
      name="viewport"
    />
    <meta content="ie=edge" http-equiv="X-UA-Compatible" />
    <title><%= title %></title>
  </head>
  <body>
    <h1>
      Welcome to
      <%= title %>
    </h1>
    <ul>
      <% categories.map((category) =>{ %>
      <li>
        <a href="/<%= category.name %>?id=<%= category.id %>"
          ><%= category.name %></a
        >
      </li>
      <% }) %>
    </ul>
  </body>
</html>
